{
  "version": 2,
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    },
    {
      "src": "frontend/api/contact.js",
      "use": "@vercel/node"
    }
  ],
  "rewrites": [
    {
      "source": "/api/contact",
      "destination": "/frontend/api/contact.js"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}